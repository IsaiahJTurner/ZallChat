# Errors get logged to /var/log/cfn-init.log. See Also /var/log/eb-tools.log
 
packages:
    yum:
        dos2unix: []
 
container_commands:
    00-command:
        command:        rm -rf /zallchat/.ebextensions
    02-command:
        command:        mkdir -p /zallchat
    02-command:
        command:        mkdir -p /zallchat/.ebextensions
 
    03-command:
        command:        cp -R .ebextensions/* /zallchat/.ebextensions/
 
    04-command:
        command:        dos2unix -k /zallchat/.ebextensions/scripts/main/app-setup.sh
 
    05-command:
        command:        chmod 700 /zallchat/.ebextensions/scripts/main/app-setup.sh
 
    06-command:
        command:        bash /zallchat/.ebextensions/scripts/main/app-setup.sh